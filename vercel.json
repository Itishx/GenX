{
  "buildCommand": "cd genx-landing && npm run build",
  "outputDirectory": "genx-landing/dist",
  "installCommand": "npm install",
  "devCommand": "cd genx-landing && npm run dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "(.*)",
      "destination": "/index.html"
    }
  ]
}
