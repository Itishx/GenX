{
  "name": "genx-monorepo",
  "version": "1.0.0",
  "description": "GenX monorepo with frontend and backend",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "workspaces": [
    "genx-landing",
    "my-express-app"
  ],
  "scripts": {
    "dev": "cd genx-landing && npm run dev",
    "build": "npm run build --workspace=genx-landing && npm run build --workspace=my-express-app",
    "start": "npm run start --workspace=my-express-app"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "@vercel/node": "^5.5.15",
    "object-hash": "^3.0.0",
    "openai": "^6.10.0"
  }
}
